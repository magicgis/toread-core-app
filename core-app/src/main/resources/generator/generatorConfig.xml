			<generatedKey column="roles_resources_id" sqlStatement="MySql" identity="true"/>
		</table>
		<table   tableName="core_resources" domainObjectName="CoreResources">
			<generatedKey column="resources_id" sqlStatement="MySql" identity="true"/>
		</table>
  </context>
</generatorConfiguration>
