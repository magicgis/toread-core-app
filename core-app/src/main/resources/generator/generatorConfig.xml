<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="C:\Users\Administrator\.m2\repository\mysql\mysql-connector-java\5.1.15\mysql-connector-java-5.1.15.jar" />
  <context id="coreApp" targetRuntime="MyBatis3" >
  
  	<property name="javaFileEncoding" value="utf-8"/>
  	<!-- 未来配置的地方 
  	<property name="javaFormatter" value=""/>
  	<property name="xmlFormatter" value=""/>
  	-->
		<commentGenerator> 
			<property name="suppressAllComments" value="true" /> 
		</commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/coreApp"
        userId="root"
        password="risen">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.toread.core.domain" targetProject="F:\myapp\core-app\src\main\java">
	    <property name="enableSubPackages" value="false" />  
	   	<property name="trimStrings" value="true" />  
    </javaModelGenerator>

    <sqlMapGenerator  targetPackage="com.toread.core.mapper"  targetProject="F:\myapp\core-app\src\main\resources">
    	<property name="enableSubPackages" value="true" />
    	<property name="trimStrings" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.toread.core.persistence"  targetProject="F:\myapp\core-app\src\main\java">
    	<property name="enableSubPackages" value="true" />
    	<property name="trimStrings" value="true" />
    </javaClientGenerator>
    
		<!--CoreAccount生成  -->
		<table   tableName="core_account" domainObjectName="CoreAccount">
			<generatedKey column="account_id" sqlStatement="MySql" identity="true"/>
